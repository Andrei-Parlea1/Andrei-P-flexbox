n and (min-width: 768px) and (max-width: 1280px) {\n    margin: 0 5%;\n  }\n  span.popup-link {\n    font-size: 14px;\n    font-weight: 200;\n    line-height: 20px;\n    color: #0CD428;\n    outline: none;\n  }\n.active {\n  label {\n    font-size: 12px;\n    line-height: 25px;\n  }\n  .errorContainer, svg {\n    display: block;\n  }\n}\n.relative {\n  position: relative;\n  display: inline-flex;\n  width: 100%;\n}\n.privacy {\n  max-width: 500px;\n  display: inline-block;\n}\nsvg {\n  fill: #f16767;\n  width: 25px;\n  z-index: 1;\n  color: #363b4a;\n  position: absolute;\n  line-height: 55px;\n  top: 15px;\n  right: 14px;\n  pointer-events: none;\n}\n.signupWrapper {\n    @media screen and (max-width: 768px) {\n      padding: 0 3%;\n    }\n    width: 100%;\n    max-width: 400px;\n    margin: auto;\n    text-align: left;\n    .text {\n      color: #fff;\n    }\n    .emailWarning, .termsWarning, .generalErr {\n      color:red;\n    }\n    .termsAndConditions {\n      display: block;\n      margin-left: 12%;\n      position: relative;\n      color: #a0a0a0;\n      font-size: 14px;\n      font-weight: 200;\n      line-height: 20px;\n    }\n    input {\n      background-color: #f7f6f6;\n      display: block;\n      position: relative;\n      width: 100%;\n      height: 55px;\n      margin-bottom: 10px;\n      outline: none;\n      -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n      -webkit-tap-highlight-color: transparent;\n      border: none;\n      -webkit-text-fill-color:#000;\n      color: black;\n      border-radius: 0;\n    }\n    input[type=text] {\n      width: 100%;\n      padding: 20px 0 0 5px;\n        @media screen and (min-width: 768px) {\n          padding: 20px 0 0 5%;\n        }\n    }\n    button {\n      -webkit-transition: all 0.2s ease-in-out;\n      -moz-transition: all 0.2s ease-in-out;\n      -ms-transition: all 0.2s ease-in-out;\n      -o-transition: all 0.2s ease-in-out;\n      transition: all 0.2s ease-in-out;\n      display: block;\n      position: relative;\n      width: auto;\n        @media screen and (min-width: 768px) {\n          width: 60%;\n        }\n      height: 55px;\n      font-size: 12px;\n      letter-spacing: 1.5px;\n      position: relative;\n      border: none;\n      color: white;\n      -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n      -webkit-tap-highlight-color: transparent;\n      -webkit-tap-hight-color: rgba(0,0,0,0);\n      outline: none;\n      background: #282828;\n    }\n    button:hover, button:focus {\n      color: #ffffff;\n      background: lighten(black, 30%);\n    }\n    label {\n      -webkit-transition: all 0.2s ease-in-out;\n      -moz-transition: all 0.2s ease-in-out;\n      -ms-transition: all 0.2s ease-in-out;\n      -o-transition: all 0.2s ease-in-out;\n      transition: all 0.2s ease-in-out;\n      opacity: 1;\n      font-size: 12px;\n      letter-spacing: .7px;\n      z-index: 1;\n      color: #363b4a;\n      position: absolute;\n      line-height: 55px;\n      top: 0;\n      left: 3px;\n      pointer-events: none;\n        @media screen and (min-width: 768px)  {\n          font-size: 14px;\n          left: 5%;\n        }\n    }\n    .categoryTitle {\n      font-size: 17px;\n      margin-top: 5%;\n    }\n    .interestCategoryTitle {\n      margin-bottom: 20px;\n      text-align: center;\n    }\n    .svgError {\n      width: 64%;\n      margin-top: 8px;\n      position: absolute;\n    }\n    .redBottom {\n      border-bottom: 2px solid red;\n    }\n    .noBottom {\n      border-bottom: 2px solid #0CD428;\n    }\n    .checkbox {\n      width: 25px;\n      float: left;\n    }\n    .errorContainer {\n      position: absolute;\n      top: 0;\n      left: 100%;\n      white-space: nowrap;\n      line-height: 55px;\n      width: auto;\n      height: 55px;\n      color:white;\n      padding: 0px 15px 0 15px;\n      background: #f16767;\n      font-size: 14px;\n      font-weight: 100;\n      letter-spacing: .6px;\n      @media screen and (max-width: 768px) {\n        display: block;\n        margin-top: -10px;\n        margin-bottom: 10px;\n        left: 0;\n        width: 100%;\n        position: relative;\n        display: block;\n      }\n    }\n    .errorContainer,svg {\n      display: none;\n    }\n    .active {\n      label {\n        font-size: 10px;\n        @media screen and (min-width: 768px) {\n          font-size: 13px;\n        }\n        line-height: 25px;\n      }\n      .errorContainer, svg{\n        display: block;\n      }\n      svg {\n        stroke: black;\n        fill: transparent;\n        stroke-linecap: round;\n        stroke-width: 5;\n      }\n    }\n}\n.hide {\n  display: none !important;\n  opacity: 0 !important;\n}\n\n/**INTEREST STEP & CONFIRM STEP**/\n.interest-section, .confirm-section {\n  .categoryTitle {\n    font-size: 17px;\n    margin: 5% 5%;\n  }\n  .interests {\n    text-align: left;\n    width: 100%;\n    // display: inline-grid;\n    display: inline-block;\n    grid-template-columns: 50% 50%;\n    grid-template-rows: 50% 50%;\n  }\n  .checkbox {\n    width: 25px;\n    display: inline;\n  }\n\n  button.signup {\n    -webkit-transition: all 0.2s ease-in-out;\n    -moz-transition: all 0.2s ease-in-out;\n    -ms-transition: all 0.2s ease-in-out;\n    -o-transition: all 0.2s ease-in-out;\n    transition: all 0.2s ease-in-out;\n    display: block;\n    position: relative;\n    width: 100%;\n    padding: 12px 22px;\n    line-height: 23px;\n    font-size: 14px;\n    letter-spacing: 1.5px;\n    border: none;\n    color: white;\n    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n    -webkit-tap-highlight-color: transparent;\n    -webkit-tap-hight-color: rgba(0,0,0,0);\n    outline: none;\n    background: #282828;\n    margin-top: 5%;\n  }\n\n  .confirmation1, .confirmation2 {\n    display: grid;\n    position: relative;\n    color: #a0a0a0;\n    font-size: 14px;\n    font-weight: 200;\n    line-height: 20px;\n  }\n}\n\n// checkbox\n\n.email-section, .interest-section {\n  .container {\n    // display: inline-block;\n    display: block;\n    position: relative;\n    margin-bottom: 12px;\n    margin-left: 0;\n    margin-right: 0;\n\n    cursor: pointer;\n    font-size: 22px;\n    -webkit-user-select: none;\n    -moz-user-select: none;\n    -ms-user-select: none;\n    user-select: none;\n    &.container1 {\n      grid-column: 1/1;\n      grid-row: 1/1;\n    }\n    &.container2 {\n      grid-column: 2/2;\n      grid-row: 1/1;\n    }\n    &.container3 {\n      grid-column: 1/1;\n      grid-row: 2/2;\n    }\n    &.container4 {\n      grid-column: 2/2;\n      grid-row: 2/2;\n    }\n  }\n  .container input {\n    position: absolute;\n    opacity: 0;\n    cursor: pointer;\n    height: 25px;\n    z-index: 2;\n  }\n  .checkmark, .checkmarkRed {\n    position: absolute;\n    top: 3px;\n    left: 0;\n    height: 25px;\n    width: 25px;\n    background-color: #eee;\n    transition: 0.4s;\n  }\n  .checkmarkRed {\n    border: 1px solid red;\n  }\n  .container:hover input ~ .checkmark {\n    background-color: #ccc;\n  }\n\n  .container input:checked ~ .checkmark {\n    background-color: #00BF60;\n  }\n\n  .checkmark:after {\n    content: "";\n    position: absolute;\n    display: none;\n  }\n  .container input:checked ~ .checkmark:after {\n    display: block;\n  }\n  .container .checkmark:after {\n    left: 9px;\n    top: 5px;\n    width: 6px;\n    height: 15px;\n    border: solid white;\n    border-width: 0 3px 3px 0;\n    -webkit-transform: rotate(45deg);\n    -ms-transform: rotate(45deg);\n    transform: rotate(45deg);\n  }\n}\n\n.interest-section {\n  .container {\n    line-height: 0;\n  }\n  input[type=checkbox] {\n    margin: 0;\n  }\n  .container input {\n    position: relative;\n    height: 21px;\n  }\n  .checkmark {\n    height: 21px;\n    width: 21px;\n    top: 6px;\n      @media only screen and (min-width: 768px) {\n        height: 21px;\n        width: 21px;\n        top: 8px;\n      }\n      @media only screen and (min-width: 1600px) {\n        top: 7px;\n      }\n      @media only screen and (min-width: 1800px) {\n        height: 18px;\n        width: 18px;\n        top: 5px;\n      }\n      @media only screen and (min-width: 2000px) {\n        top: 9px;\n      }\n  }\n  .container .checkmark:after {\n    left: 8px;\n    top: 8%;\n    width: 6px;\n    @media only screen and (min-width: 1800px) {\n      left: 6px;\n      top: 0;\n      width: 7px;\n    }\n  }\n  .container p {\n    // margin-left: 10%;\n    font-size: 15px;\n    hyphens: auto;\n    display: inline;\n  }\n  span.popup-link {\n    font-size: 14px;\n    font-weight: 200;\n    line-height: 20px;\n    color: #00BF60;\n  }\n  @media only screen and (min-width: 768px) {\n    .container {\n      line-height: 0;\n    }\n    .interests {\n      padding: 0 10%;\n    }\n    .container p {\n      font-size: 14px;\n    }\n  }\n  @media only screen and (min-width: 1280px) {\n\n    .interests {\n      padding: 0;\n    }\n  }\n  @media only screen and (min-width: 1360px) {\n    .interests {\n      padding: 0 15%;\n    }\n    .continue {\n      padding: 0 15%;\n    }\n  }\n}\n}\n'],sourceRoot:""}]),n.Z=o},73935:function(e,n,t){"use strict";!function e(){if("undefined"!=typeof __REACT_DEVTOOLS_GLOBAL_HOOK__&&"function"==typeof __REACT_DEVTOOLS_GLOBAL_HOOK__.checkDCE)try{__REACT_DEVTOOLS_GLOBAL_HOOK__.checkDCE(e)}catch(e){}}(),e.exports=t(64448)},67294:function(e,n,t){"use strict";e.exports=t(72408)},84121:function(e,n,t){"use strict";var i=t(93379),r=t.n(i),a=t(73109),o={insert:"head",singleton:!1};r()(a.Z,o),a.Z.locals},24175:function(e,n,t){"use strict";var i=t(93379),r=t.n(i),a=t(27222),o={insert:"head",singleton:!1};r()(a.Z,o),a.Z.locals},19395:function(e,n,t){e.exports=t(72107)}}]);
